{
    "name": "vansilich/kernel-search",
    "description": "Part of kernel which handles Elasticsearch",
    "license": "MIT",
    "version": "0.0.0",
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "ext-mbstring": "*",
        "elasticsearch/elasticsearch": "^8.7",
        "vlucas/phpdotenv": "^5.5",
        "symfony/http-client": "^6.2",
        "psr/http-client": "^1.0",
        "nyholm/psr7": "^1.5",
        "guzzlehttp/guzzle": "^7.7"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "friendsofphp/php-cs-fixer": "^3.13",
        "dg/bypass-finals": "^1.4",
        "phpstan/phpstan": "^1.10"
    },
    "suggest": {
        "psr/http-client-implementation": "Required for using this package"
    },
    "autoload": {
        "psr-4": {
            "AvySearch\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AvySearch\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
